   "source": [
    "num = int(input(\"Enter The Number\"))\n",
    "num = str(num)\n",
    "\n",
    "ln = len(num)\n",
    "s=0\n",
    "for i in num :\n",
    "    s += int(i) ** ln\n",
    "if s == int(num):\n",
    "    print('Armstrong Number')\n",
    "\n",
    "else:\n",
    "    print('not armstrong')"
   ]
